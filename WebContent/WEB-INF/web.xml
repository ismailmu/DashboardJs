<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>DashboardJs</display-name>
  <listener>
    <description>contextListener</description>
    <listener-class>com.btpns.dashboardjs.server.DashboardJsContextListener</listener-class>
  </listener>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>WismaServlet</display-name>
    <servlet-name>WismaServlet</servlet-name>
    <servlet-class>com.btpns.dashboardjs.server.wisma.WismaServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WismaServlet</servlet-name>
    <url-pattern>/WismaServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>HelpdeskServlet</display-name>
    <servlet-name>HelpdeskServlet</servlet-name>
    <servlet-class>com.btpns.dashboardjs.server.helpdesk.HelpdeskServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelpdeskServlet</servlet-name>
    <url-pattern>/HelpdeskServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>MenuServlet</display-name>
    <servlet-name>MenuServlet</servlet-name>
    <servlet-class>com.btpns.dashboardjs.server.menu.MenuServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MenuServlet</servlet-name>
    <url-pattern>/MenuServlet</url-pattern>
  </servlet-mapping>
</web-app>